# DMM API設定
# DMM API設定（AV女優名の大量取得用）
DMM_API_ID=your_dmm_api_id_here
DMM_AFFILIATE_ID=your_dmm_affiliate_id_here

# Firebase設定
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id

# その他の環境変数
NODE_ENV=development

# RevenueCat (optional: in-app purchases)
EXPO_PUBLIC_RC_API_KEY=your_revenuecat_api_key
# Enable dev fallback to simulate payments without real charge (true/false)
EXPO_PUBLIC_PAYMENTS_DEV_MODE=false
