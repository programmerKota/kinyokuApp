## Expo public envs used at runtime (safe to embed)
## Copy to .env, .env.local, or set in EAS Secrets

# RevenueCat
# Prefer platform-specific Public SDK Keys; single key is fallback
EXPO_PUBLIC_RC_IOS_PUBLIC_API_KEY=
EXPO_PUBLIC_RC_ANDROID_PUBLIC_API_KEY=
# Optional fallback (used if platform-specific keys are not set)
# EXPO_PUBLIC_RC_API_KEY=

# Firebase (Production) - Console > Project Settings > Web app config
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=

# Optional
# EXPO_PUBLIC_FIREBASE_FUNCTIONS_REGION=us-central1
# EXPO_PUBLIC_ENABLE_FUNCTIONS=true
