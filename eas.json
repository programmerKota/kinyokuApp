{
  "cli": {
    "version": ">= 12.6.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "1",
        "HUSKY": "0"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "1",
        "HUSKY": "0"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "1",
        "HUSKY": "0"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6753818762"
      }
    }
  }
}
